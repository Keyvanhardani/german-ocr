{
    "name": "keyvan/german-ocr",
    "description": "High-performance German document OCR - Local & Cloud API",
    "version": "0.5.1",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Keyvan.ai",
            "email": "support@keyvan.ai",
            "homepage": "https://keyvan.ai"
        }
    ],
    "homepage": "https://german-ocr.de",
    "keywords": [
        "ocr",
        "german",
        "document",
        "pdf",
        "invoice",
        "text-extraction",
        "vision",
        "ai"
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "GermanOCR\\": "composer/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GermanOCR\\Tests\\": "composer/tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
